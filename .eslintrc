{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "@cabe",
    "@cabe/typescript",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "react/prop-types": "off"
  }
}
