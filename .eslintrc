{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "@cabe",
    "@cabe/typescript",
    "plugin:react/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "react-hooks"
  ],
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error"
  }
}
